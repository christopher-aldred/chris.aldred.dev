    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-junit": "^16.0.0",
    "postcss": "^8.4.41",
    "react-test-renderer": "^18.3.1"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.3",
    "@tinacms/cli": "^1.6.4",
